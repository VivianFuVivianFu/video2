{
  "rewrites": [
    {
      "source": "/",
      "destination": "/demo/index.html"
    },
    {
      "source": "/video",
      "destination": "/demo/luma_producthunt.mp4"
    }
  ],
  "headers": [
    {
      "source": "/demo/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.mp4)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ]
}
